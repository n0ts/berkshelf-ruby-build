{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:GetObject",
        "s3:PutObjectAcl",
        "s3:PutObject"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::simple-ruby-build/*"
    },
    {
      "Action": [
         "s3:ListBucketMultipartUploads",
         "s3:GetBucketLocation"
       ],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::simple-ruby-build"
    }
  ]
}